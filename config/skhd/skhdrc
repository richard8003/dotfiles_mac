alt - r : open -a "/Applications/Ghostty.app"
alt - s : open -a "/Applications/Google Chrome.app" 
alt - a : open -a "/Applications/Spotify.app" 
alt - t : open -a "/Applications/Discord.app" 
alt - p : open -a "/Applications/WhatsApp.app" 
alt - w : open -a "/Applications/Firefox.app"
alt - d : open -a "Messages"


